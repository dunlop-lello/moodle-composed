{
    "name": "dunloplello/composed-moodle",
    "type": "package",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dunlop-lello/moodle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dunlop-lello/kaltura_moodle_plugin.git"
        }
    ],
    "require": {
        "moodle/moodle": "MOODLE_31_MDL-54834-dev",
        "kaltura/moodle_plugin": "MOODLE_30_MDL-54834-dev"
    },
    "scripts": {
        "pre-install-cmd": "DunlopLello\\ComposedMoodle::preInstall",
        "pre-update-cmd": "DunlopLello\\ComposedMoodle::preUpdate",
        "post-install-cmd": "DunlopLello\\ComposedMoodle::postInstall",
        "post-update-cmd": "DunlopLello\\ComposedMoodle::postUpdate"
    },
    "autoload": {
        "psr-4": {
            "DunlopLello\\": "src/"
        }
    }
}
